{
    "name": "peanut-ui",
    "version": "0.2.0",
    "private": true,
    "engines": {
        "node": ">=21.1.0 <22.0.0 || ^18.18.0 || ^20.9.0"
    },
    "scripts": {
        "dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" next dev --turbo",
        "dev:clean": "rm -rf .next && NODE_OPTIONS=\"--max-old-space-size=8192\" next dev --turbo",
        "dev:fallback": "next dev",
        "build": "next build --webpack",
        "start": "next start",
        "lint": "next lint",
        "knip": "knip",
        "format": "prettier --write .",
        "analyze": "ANALYZE=true next build",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "script": "NODE_OPTIONS=\"--experimental-json-modules\" tsx"
    },
    "dependencies": {
        "@dicebear/collection": "^9.2.2",
        "@dicebear/core": "^9.2.2",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@headlessui/react": "^2.2.9",
        "@headlessui/tailwindcss": "^0.2.1",
        "@justaname.id/react": "0.3.180",
        "@justaname.id/sdk": "0.2.177",
        "@mui/icons-material": "^7.3.6",
        "@mui/material": "^7.3.6",
        "@radix-ui/react-accordion": "^1.2.12",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slider": "^1.3.5",
        "@radix-ui/react-tabs": "^1.1.13",
        "@reduxjs/toolkit": "^2.5.0",
        "@reown/appkit": "1.6.9",
        "@reown/appkit-adapter-wagmi": "1.6.9",
        "@safe-global/safe-apps-sdk": "^9.1.0",
        "@sentry/nextjs": "^8.39.0",
        "@serwist/next": "^9.0.10",
        "@simplewebauthn/browser": "^8.3.7",
        "@squirrel-labs/peanut-sdk": "^0.6.2",
        "@tanstack/react-query": "5.8.4",
        "@typeform/embed-react": "^3.20.0",
        "@vercel/analytics": "^1.4.1",
        "@wagmi/core": "2.19.0",
        "@zerodev/passkey-validator": "^5.6.0",
        "@zerodev/sdk": "5.5.0",
        "autoprefixer": "^10.4.20",
        "canvas-confetti": "^1.9.3",
        "classnames": "^2.5.1",
        "d3-force": "^3.0.0",
        "embla-carousel-react": "^8.6.0",
        "ethers": "5.7.2",
        "framer-motion": "^11.11.17",
        "i18n-iso-countries": "^7.13.0",
        "iban-to-bic": "^1.4.0",
        "js-cookie": "^3.0.5",
        "jsqr": "^1.4.0",
        "next": "16.0.10",
        "nuqs": "^2.8.6",
        "pix-utils": "^2.8.2",
        "pulltorefreshjs": "^0.1.22",
        "qr-scanner": "^1.4.2",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "react-fast-marquee": "^1.6.5",
        "react-force-graph-2d": "^1.25.10",
        "react-hook-form": "^7.53.2",
        "react-onesignal": "^3.2.3",
        "react-qr-code": "^2.0.15",
        "react-redux": "^9.2.0",
        "react-tooltip": "^5.28.0",
        "redux": "^5.0.1",
        "siwe": "^2.3.2",
        "tailwind-merge": "^1.14.0",
        "tailwind-scrollbar": "^3.1.0",
        "use-haptic": "^1.1.11",
        "validator": "^13.12.0",
        "vaul": "^1.1.2",
        "viem": "^2.22.0",
        "wagmi": "2.16.3"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^16.1.1",
        "@serwist/build": "^9.0.10",
        "@size-limit/preset-app": "^11.2.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^16.1.0",
        "@types/canvas-confetti": "^1.9.0",
        "@types/jest": "^29.5.12",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "20.4.2",
        "@types/pulltorefreshjs": "^0.1.7",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/validator": "^13.12.2",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-transform-stub": "^2.0.0",
        "knip": "^5.37.1",
        "postcss": "^8.4.49",
        "postcss-import": "^16.1.0",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "serwist": "^9.0.10",
        "size-limit": "^11.2.0",
        "tailwindcss": "^3.4.15",
        "ts-jest": "^29.1.2",
        "tsx": "^4.19.3",
        "typescript": "^5.6.3"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest",
                {
                    "tsconfig": "tsconfig.test.json",
                    "useESM": true
                }
            ]
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(@wagmi|wagmi|viem|@viem|@squirrel-labs|@reown|@walletconnect|@justaname\\.id|@zerodev|permissionless)/)"
        ],
        "moduleNameMapper": {
            "\\.(svg|png|jpg|jpeg|gif)$": "jest-transform-stub",
            "^@/config/wagmi\\.config$": "<rootDir>/src/utils/__mocks__/wagmi-config.ts",
            "^wagmi/chains$": "<rootDir>/src/utils/__mocks__/wagmi.ts",
            "^@squirrel-labs/peanut-sdk$": "<rootDir>/src/utils/__mocks__/peanut-sdk.ts",
            "^@reown/appkit/react$": "<rootDir>/src/utils/__mocks__/reown-appkit.ts",
            "^@justaname\\.id/react$": "<rootDir>/src/utils/__mocks__/justaname.ts",
            "^next/cache$": "<rootDir>/src/utils/__mocks__/next-cache.ts",
            "^@zerodev/sdk(.*)$": "<rootDir>/src/utils/__mocks__/zerodev-sdk.ts",
            "^@simplewebauthn/browser$": "<rootDir>/src/utils/__mocks__/simplewebauthn-browser.ts",
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.ts"
        ],
        "testMatch": [
            "**/__tests__/**/*.test.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)"
        ],
        "extensionsToTreatAsEsm": [
            ".ts",
            ".tsx"
        ]
    },
    "resolutions": {
        "@wagmi/core": "2.19.0",
        "viem": "^2.22.0",
        "wagmi": "2.16.3"
    },
    "size-limit": [
        {
            "path": ".next/static/chunks/*.js",
            "limit": "500 KB"
        }
    ]
}
